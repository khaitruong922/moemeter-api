{
	"compilerOptions": {
		"target": "es2021",
		"lib": ["es2021"],
		"jsx": "react-jsx",
		"jsxImportSource": "hono/jsx",
		"module": "es2022",
		"moduleResolution": "node",
		"types": ["@cloudflare/workers-types/2023-07-01", "@types/jest"],
		"resolveJsonModule": true,
		"allowJs": true,
		"checkJs": false,
		"noEmit": true,
		"isolatedModules": true,
		"allowSyntheticDefaultImports": true,
		"forceConsistentCasingInFileNames": true,
		"skipLibCheck": true
	}
}
